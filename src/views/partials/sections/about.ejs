<section id="profile" class="section bg-white dark:bg-gray-900">
  <div class="container mx-auto max-w-3xl">
    <div class="animate-on-scroll">
      <h2 class="section-title">Perfil</h2>
    </div>

    <div class="animate-on-scroll">
      <% data.about.description.forEach(function(paragraph) { %>
        <p class="text-lg md:text-xl text-gray-700 dark:text-gray-300 leading-relaxed font-light">
          <%= paragraph %>
        </p>
      <% }); %>
    </div>
  </div>
</section>
